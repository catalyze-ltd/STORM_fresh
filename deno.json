{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.6.8/",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "$std/": "https://deno.land/std@0.216.0/",

    "fresh-session": "https://deno.land/x/fresh_session@0.2.2/mod.ts",
    "secure-compare": "https://deno.land/x/secure_compare@1.0.0/mod.ts",

    "unocss": "npm:unocss@~0.58.5",
    "@unocss/core": "npm:@unocss/core@~0.58.5",
    "@unocss/preset-mini": "npm:@unocss/preset-mini@~0.58.5",
    "@unocss/preset-mini/": "npm:/@unocss/preset-mini@~0.58.5/",
    "@unocss/preset-uno": "npm:@unocss/preset-uno@~0.58.5",
    "@unocss/transformer-variant-group": "npm:@unocss/transformer-variant-group@~0.58.5",

    "class-variance-authority": "https://esm.sh/class-variance-authority@0.7.0",

    "react": "https://esm.sh/preact@10.19.6/compat",
    "react-dom": "https://esm.sh/preact@10.19.6/compat",
    "@types/react": "https://esm.sh/preact@10.19.6/compat",

    "@radix-ui/react-alert-dialog": "https://esm.sh/@radix-ui/react-alert-dialog@1.0.4?external=react,react-dom,@types/react&target=es2022",
    "@radix-ui/react-avatar": "https://esm.sh/@radix-ui/react-avatar@1.0.3?external=react,react-dom,@types/react&target=es2022",
    "@radix-ui/react-slot": "https://esm.sh/@radix-ui/react-slot@1.0.2?external=react,react-dom,@types/react&target=es2022",
    "@radix-ui/react-dialog": "https://esm.sh/@radix-ui/react-dialog@1.0.4?external=react,react-dom,@types/react&target=es2022",
    "@radix-ui/react-dropdown-menu": "https://esm.sh/@radix-ui/react-dropdown-menu@2.0.5?external=react,react-dom,@types/react&target=es2022",
    "@radix-ui/react-label": "https://esm.sh/@radix-ui/react-label@2.0.2?external=react,react-dom,@types/react&target=es2022",
    "@radix-ui/react-select": "https://esm.sh/@radix-ui/react-select@2.0.0?external=react,react-dom,@types/react&target=es2022",
    "@radix-ui/react-separator": "https://esm.sh/@radix-ui/react-separator@1.0.3?external=react,react-dom,@types/react&target=es2022",
    "@radix-ui/react-tabs": "https://esm.sh/@radix-ui/react-tabs@1.0.4?external=react,react-dom,@types/react&target=es2022",

    "@tanstack/table-core": "https://esm.sh/@tanstack/table-core@8.10.3?target=es2022",
    "@tanstack/react-table": "https://esm.sh/@tanstack/react-table@8.10.3?external=react,@types/react,@tanstack/table-core&target=es2022",

    "cmdk": "https://esm.sh/cmdk@0.2.0?external=react,react-dom,@types/react&target=es2022",

    "lucide-preact": "https://esm.sh/lucide-preact@0.303.0?external=preact&target=es2022",

    "drizzle-orm": "https://esm.sh/drizzle-orm@0.30.8?external=postgres,bun-types",
