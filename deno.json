    "postgres": "https://deno.land/x/postgresjs@v3.4.4/mod.js",

    "zod": "https://deno.land/x/zod@v3.22.4/mod.ts",
    "zod-form-data": "https://esm.sh/zod-form-data@2.0.2?external=zod",
    "zod-validation-error": "https://esm.sh/zod-validation-error@1.5.0?external=zod",

    "markdown-it": "https://esm.sh/markdown-it@13.0.2",

    "xlsx": "https://esm.sh/xlsx@0.18.5",

    "date-fns": "https://esm.sh/date-fns@3.3.1",

